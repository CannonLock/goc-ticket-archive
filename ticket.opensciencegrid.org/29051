[root@fifebatchgpvmhead1 gwms-frontend]# condor_status -any -pool gfactory-1.t2.ucsd.edu -constraint &#39;glideinmytype==&#34;glidefactory&#34;&#39; -constraint &#39;glidein_site == &#34;SMU_HPC&#34;&#39; -af glidein_supported_vos
