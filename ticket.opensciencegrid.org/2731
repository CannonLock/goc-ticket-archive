#!/bin/bash

# ------------   user environment    -----------

if [ -f $GLOBUS_LOCATION/etc/grid3-info.conf ]; then

source $GLOBUS_LOCATION/etc/grid3-info.conf

fi

if [ -f $GRID3_BASE_DIR/setup.sh ]; then

source $GRID3_BASE_DIR/setup.sh

fi

if [ -f $OSG_GRID/setup.sh ]; then

source $OSG_GRID/setup.sh

fi

<div id='show_1103035408' class=''><button class="btn">Show More</button></div><div class='detail hidden' id='detail_1103035408'># ------------   General configuration   -----------

# where to build the temp work dir

gridwork_basedir=${OSG_WN_TMP}/tmp_hpi_1109_${RANDOM}_${RANDOM}/AlpgenWork

# absolute id for the job during a period of time

LOG_id=10

# the id that relates to specific physics process (analaysis)

process_id=10

# another way to help identify the job

today=1109

# physics process

datacard=Z0j

# what executable

pythia_exec=kine_make_ntpl_alpgen_match.exe

famos_exec=newWBFhiggs

# where to find the input data files (in a http source)

gridinput=<a href='srm&#58;//t2data2.t2.ucsd.edu&#58;8443/data4/cms/userdata/hpi/OSG_Production/stage_file' target='_blank' rel='nofollow'>srm&#58;//t2data2.t2.ucsd.edu&#58;8443/data4/cms/userdata/hpi/OSG_Production/stage_file</a>

# Ntuple file&#39;s name

ntupfile=${datacard}_${process_id}.ntpl

# number of events in CMKIN

cmkineventnum=10002

# number of events in FAMOS

famoseventnum=10000

# ------------- storage configuration --------------

myDcache=<a href='srm&#58;//t2data2.t2.ucsd.edu&#58;8443/data4/cms/userdata/hpi/WZjets/FAMOS/site_test_1109' target='_blank' rel='nofollow'>srm&#58;//t2data2.t2.ucsd.edu&#58;8443/data4/cms/userdata/hpi/WZjets/FAMOS/site_test_1109</a>

# ------------- prepare --------------------

if [ ! -e ${gridwork_basedir} ]

then

mkdir -p ${gridwork_basedir}

fi

cd ${gridwork_basedir}

srmcp -streams_num=1 ${gridinput}/cmkin.tar.gz
file&#58;//localhost/${gridwork_basedir}/cmkin.tar.gz

srmcp -streams_num=1 ${gridinput}/famos.tar.gz
file&#58;//localhost/${gridwork_basedir}/famos.tar.gz

tar zxf cmkin.tar.gz

tar zxf famos.tar.gz

# --------- get CMKIN ------------

cd ${gridwork_basedir}/cmkin

srmcp -streams_num=1 ${gridinput}/${datacard}/${datacard}_${process_id}.unw
file&#58;//localhost/&#96;pwd&#96;/${datacard}_${process_id}.unw

srmcp -streams_num=1 ${gridinput}/${datacard}_unw.par
file&#58;//localhost/&#96;pwd&#96;/${datacard}_unw.par

ln -s ${datacard}_${process_id}.unw alpgen.unw

ln -s ${datacard}_unw.par alpgen_unw.par

# ------------- CMKIN ----------------------

source change_parameter

export LD_LIBRARY_PATH=${LD_LIBRARY_PATH}&#58;.

# ./${pythia_exec} &#62; log &#60; Cmkin_Config

#srmcp file&#58;//localhost/${gridwork_basedir}/cmkin/${ntupfile} ${myDcache}/${ntupfile}

# ------------- FAMOS ----------------------

export CMS_PATH=${OSG_APP}/cmssoft/cms

source $CMS_PATH/setup/bashrc

export SCRAM_ARCH=slc3_ia32_gcc323

export CERN=$CMS_PATH/lcg/external/cern

export CERN_LEVEL=2004/$SCRAM_ARCH

export CERN_ROOT=$CERN/$CERN_LEVEL

export GROUP_DIR=$CMS_PATH/setup

#export CMS_SYS=&#96;$CMS_PATH/utils/fake-sys&#96;

export CMS_SYS=slc3_ia32_gcc323

export PATH=${CMS_PATH}/bin/${CMS_SYS}&#58;${CMS_PATH}/utils&#58;${CERN_ROOT}/bin&#58;$PATH

cd ../famos

scramv1 project FAMOS FAMOS_1_4_0

if [ ! -e FAMOS_1_4_0 ]

then

mkdir -p FAMOS_1_4_0

fi

mkdir FAMOS_1_4_0/analysis

cp orcarc.higgs FAMOS_1_4_0/analysis

mkdir -p FAMOS_1_4_0/lib/slc3_ia32_gcc323

cp *.so FAMOS_1_4_0/lib/slc3_ia32_gcc323

mkdir -p FAMOS_1_4_0/bin/slc3_ia32_gcc323

cp ${famos_exec} FAMOS_1_4_0/bin/slc3_ia32_gcc323

cp change_parameter FAMOS_1_4_0/analysis

cd FAMOS_1_4_0/analysis

#ln -s ${gridwork_basedir}/cmkin/${datacard}_${process_id}.ntpl cmkin.ntpl

source change_parameter

eval &#96;scramv1 runtime -sh&#96;

#${famos_exec} -c Orcarc.Higgs &#62;&amp; log

#srmcp
file&#58;//localhost/${gridwork_basedir}/famos/FAMOS_1_4_0/analysis/newWBFhiggs.root
${myDcache}/newWBFhiggs_${process_id}.root

scramv1 list &#62; logfile

srmcp file&#58;//localhost/${gridwork_basedir}/famos/FAMOS_1_4_0/analysis/logfile
${myDcache}/newWBFhiggs_${process_id}.log

#cp newWBFhiggs.root ${gridwork_basedir}/newWBFhiggs_${process_id}.root

#cp newWBFhiggs.root /tmp/newWBFhiggs_${process_id}.root

cd /tmp

# ------------- Finish ---------------------

rm -r ${gridwork_basedir}
